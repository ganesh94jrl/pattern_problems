/*inverted pattern
1 2 3 4 5   //row=> 1 to n(n=5)
1 2 3 4     //column=> n+1-row number
1 2 3
1 2 
1 */


#include<iostream>
using namespace std;
int main(){
    int i,j,n;
    cin>>n;
    for(i=1;i<=n;i++){
        for(j=1;j<=n+1-i;j++){
            cout<<j<<" ";
        }
        cout<<endl;
    }
    return 0;
}
